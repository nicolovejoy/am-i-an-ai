{
  "name": "frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest --coverage",
    "db:migrate": "node -r ts-node/register -e \"import('./src/lib/migrations').then(m => m.runMigrations())\"",
    "db:rollback": "node -r ts-node/register -e \"import('./src/lib/migrations').then(m => m.rollbackMigration())\"",
    "db:seed": "node -r ts-node/register -e \"import('./src/lib/seedData').then(s => s.seedDatabase())\"",
    "db:reset": "node -r ts-node/register -e \"import('./src/lib/seedData').then(s => s.resetDatabase())\""
  },
  "dependencies": {
    "@hookform/resolvers": "4.1.3",
    "@tanstack/react-query": "5.68.0",
    "@tanstack/react-query-devtools": "5.68.0",
    "amazon-cognito-identity-js": "6.3.12",
    "eslint-plugin-jest": "28.13.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-testing-library": "7.4.0",
    "jest-axe": "10.0.0",
    "next": "14.2.28",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.55.0",
    "react-icons": "5.5.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "20.12.7",
    "@types/pg": "^8.11.10",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.28",
    "globals": "^13.24.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.18.0"
  }
}
